{
  "name": "jambagecom/base-excel",
  "type": "typo3-cms-extension",
  "description": "This provides the PhpSpreadsheet library, formerly phpExcel, from phpOffice.",
  "keywords": [
    "TYPO3",
    "extension",
    "excel",
    "phpOffice"
  ],
  "support": {
    "issues": "https://github.com/franzholz/base_excel/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
        "name": "Maarten Balliauw",
        "homepage": "http://blog.maartenballiauw.be"
    },
    {
        "name": "Mark Baker",
        "homepage": "http://markbakeruk.net"
    },
    {
        "name": "Franck Lefevre",
        "homepage": "http://rootslabs.net"
    },
    {
        "name": "Erik Tilt"
    },
    {
        "name": "Adrien Crivelli"
    },
    {
      "name": "Franz Holzinger",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": ">=8.5.0,<9.99.99",
       "php": "^7.3 || ^8.0",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "ext-xmlreader": "*",
        "ext-xmlwriter": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "jambagecom/base-complex": "^0.0",
        "jambagecom/base-html": "^0.0",
        "jambagecom/base-matrix": "^0.0",
        "jambagecom/base-zip": "^0.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
   },
  "autoload": {
    "psr-4": {
        "PhpOffice\\PhpSpreadsheet\\": "Vendor/PhpSpreadsheet/src/PhpSpreadsheet",
        "JambageCom\\BaseExcel\\": "Classes"
    }
  },
  "replace": {
    "typo3-ter/base-excel": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "base_excel"
    }
  }
}
