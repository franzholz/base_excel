{
  "name": "jambagecom/base-excel",
  "type": "typo3-cms-extension",
  "description": "This provides the PhpSpreadsheet library, formerly phpExcel, from phpOffice.",
  "keywords": [
    "TYPO3",
    "extension",
    "excel",
    "phpOffice"
  ],
  "support": {
    "issues": "https://github.com/franzholz/base_excel/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
        "name": "Maarten Balliauw",
        "homepage": "http://blog.maartenballiauw.be"
    },
    {
        "name": "Mark Baker",
        "homepage": "http://markbakeruk.net"
    },
    {
        "name": "Franck Lefevre",
        "homepage": "http://rootslabs.net"
    },
    {
        "name": "Erik Tilt"
    },
    {
      "name": "Franz Holzinger",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms-core": ">=6.2.0,<9.99.99",
    "php": "^5.6|^7.0",
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-SimpleXML": "*",
    "ext-xml": "*",
    "ext-xmlreader": "*",
    "ext-xmlwriter": "*",
    "ext-zip": "*",
    "ext-zlib": "*",
    "psr/simple-cache": "^1.0"
  },
  "autoload": {
    "psr-4": {
        "PhpOffice\\PhpSpreadsheet\\": "Vendor/PhpSpreadsheet/src/PhpSpreadsheet"
    }
  },
  "replace": {
    "base_excel": "self.version",
    "typo3-ter/base-excel": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "base_excel"
    }
  }
}
